{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "vmName": {
            "value": "vmlab01"
        },
        "vmCount": {
            "value": 2
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "default-allow-rdp",
                    "properties": {
                        "priority": 1000,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceApplicationSecurityGroups": [],
                        "destinationApplicationSecurityGroups": [],
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "VMSubnet"
        },
        "subnetAddressPrefix": {
            "value":"10.0.0.0/24"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "privateIpAddressIP": {
            "value": "10.0.0."
        },
        "pipDeleteOption": {
            "value": "Delete"
        },
        "DiskType": {
            "value": "StandardSSD_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_B2s"
        },
        "nicDeleteOption": {
            "value": "Delete"
        },
        "adminUsername": {
            "value": "lab-user"
        },
        "adminPassword": {
            "reference": {
                "keyVault": {
                  "id": "/subscriptions/aeda07d9-84a0-4974-b345-dfbe9df0dad4/resourceGroups/VMLabGroup/providers/Microsoft.KeyVault/vaults/vmlab-pass"
                },
                "secretName": "vm-lab01"
              }
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        }
        
        
    }
}